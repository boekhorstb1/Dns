{
    "name": "horde/dns",
    "description": "Horde Domain Name System library",
    "type": "library",
    "homepage": "https://www.horde.org",
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "Ralf Lang",
            "email": "lang@b1-systems.de",
            "role": "lead"
        },
        {
            "name": "Diana Hille",
            "email": "hille@b1-systems.de",
            "role": "lead"
        }
    ],
    "time": "2022-07-11",
    "repositories": [
        {
            "type": "composer",
            "url": "https://horde-satis.maintaina.com"
        }
    ],
    "require": {
        "php": "^7 || ^8",
        "horde/rdo": "^3 || dev-FRAMEWORK_6_0",
        "aws/aws-sdk-php": "^3"
    },
    "require-dev": {
        "horde/test": "^3 || dev-FRAMEWORK_6_0"
    },
    "suggest": {},
    "autoload": {
        "psr-0": {
            "Horde_DNS": "lib/"
        },
        "psr-4": {
            "Horde\\Dns\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Horde\\Dns\\Test\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "horde/horde-installer-plugin": true
        }
    },
    "minimum-stability": "dev"
}
